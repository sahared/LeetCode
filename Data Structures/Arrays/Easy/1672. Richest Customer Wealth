1672. Richest Customer Wealth
**************************************

class Solution {
    public int maximumWealth(int[][] accounts) {
        int ans = Integer.MIN_VALUE;
	for(int m = 0; m< accounts.length; m++){
		int sum =0;
		for(int n = 0; n < accounts[m].length; n++ ){
			sum += accounts[m][n];

		}
		if(sum > ans)
			ans = sum;

	}
	return ans;
        
    }
}
