2161. Partition Array According to Given Pivot
*******************************************************

class Solution{
public int[] pivotArray(int[] nums, int pivot){
int[] res = new int[nums.length];
int L=0, R=0, res=0;
for(int i =0; i<nums.length; i++){
if(nums[i]<pivot)
res[L++] = nums[i];
if(nums[i]>pivot)
res[R--] = nums[nums.length - 1 -i];
}
while(L<=R){
res[L++] =pivote;
res[R--} = pivote;
}
return res;
}
}
