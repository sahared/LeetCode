287. Find the Duplicate Number
****************************************

class Solution {
    public int findDuplicate(int[] nums) {
        Arrays.sort(nums);
        int len = nums.length;
        for(int i=0; i<len; i++){
            if(nums[i+1] ==nums [i]){
          return nums[i];
      }
    }
        return len;
}
}
